  "keywords": [
    "kapersoft",
    "sharefile-api",
    "sharefile",
    "api",
    "php"
  ],
  "homepage": "https://github.com/kapersoft/sharefile-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Willem Kaper",
      "email": "kapersoft@gmail.com",
      "homepage": "https://kapersoft.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0|^8.0",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "require-dev": {
    "larapack/dd": "1.*",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Kapersoft\\Sharefile\\": "src"
